{
  "name": "infinite-ui-v2",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "use": "node scripts/setup/project.js",
    "svelte-kit": "svelte-kit",
    "proxy": "node proxy.cjs",
    "dev:kit": "run-p -r \"proxy\" \"svelte-kit dev\" --",
    "dev": "run-s \"use -- {1}\" \"dev:kit\" --",
    "build": "run-s \"use -- {1}\" \"svelte-kit build\" --",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json --threshold warning",
    "lint": "eslint --cache --ignore-path .gitignore .",
    "format": "prettier --plugin-search-dir=. --write .",
    "format:check": "prettier --plugin-search-dir=. --check .",
    "test": "jest --coverage",
    "tdd": "jest --watch",
    "e2e": "cypress run --browser chrome",
    "fclt": "pnpm format && pnpm check && pnpm lint && pnpm test",
    "build-storybook": "build-storybook",
    "start-storybook": "start-storybook -p 6006",
    "storybook": "run-s \"use -- {1}\" \"start-storybook\" --",
    "chromatic": "chromatic"
  },
